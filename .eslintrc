{
    "env": {
        "browser": false,
        "node": true,
        "commonjs": true
    },
    "extends": [
        "airbnb-base",
        "prettier",
        "plugin:node/recommended"
    ],
    "plugins": [
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error",
        "no-unused-vars": "warn",
        "function-call-argument-newline": "off"
    }
}